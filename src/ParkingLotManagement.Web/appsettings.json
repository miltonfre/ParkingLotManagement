{
  "DatabaseSettings": {
    "ConnectionString": "Data Source=(localdb)\\mssqllocaldb;Initial Catalog=ParkingDB; Integrated Security=true",
    //CS to connect the database automatically
    "InitialConnectionString": "Data Source=(localdb)\\mssqllocaldb;Initial Catalog=master; Integrated Security=true"
  },
  "InitialSettings": {
    // 0 - If the first run does not want create the new databas
    // 1 - Create the database at begining
    "CreateInitialDatabase": "1"
  },
  "CustomSettings": {
    "CapacitySpots": "25",
    "HourlyFee": "15"
  },


  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
